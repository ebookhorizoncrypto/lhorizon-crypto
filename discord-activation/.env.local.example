# URL de votre site (pour le redirect_uri)
NEXT_PUBLIC_URL=https://votre-site.com
# ID Client Discord (Public)
NEXT_PUBLIC_DISCORD_CLIENT_ID=votre_client_id_discord

# Secrets Discord (Server-side only)
DISCORD_CLIENT_SECRET=votre_client_secret
DISCORD_BOT_TOKEN=votre_bot_token_discord
DISCORD_GUILD_ID=votre_server_id_discord

# IDs des Rôles Discord
DISCORD_ROLE_VIP_ID=123456789
DISCORD_ROLE_PRO_ID=123456789
DISCORD_ROLE_SOLO_ID=123456789
DISCORD_ROLE_MEMBER_ID=123456789

# Supabase (Admin Key requise pour écrire dans la table users si RLS activé ou pour sécu)
SUPABASE_URL=https://xyz.supabase.co
SUPABASE_SERVICE_KEY=votre_service_role_key
